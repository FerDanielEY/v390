<style>
  header-drawer {
    justify-self: start;
    margin-left: -1.2rem;
  }
  #header-custom > header-drawer {
    justify-self: end;
  }
  #header-custom > div {
    align-items: baseline;
  }
  @media screen and (min-width: 990px) {
    header-drawer {
      display: none;
    }
  }
  .menu-drawer-container {
    display: flex;
  }
  .list-menu {
    list-style: none;
    padding: 0;
    margin: 0;
  }
  .list-menu--inline {
    display: inline-flex;
    flex-wrap: wrap;
  }
  summary.list-menu__item {
    padding-right: 2.7rem;
  }
  .list-menu__item {
    display: flex;
    align-items: center;
    line-height: 1.3;
  }
  .list-menu__item--link {
    text-decoration: none;
    padding-bottom: 1rem;
    padding-top: 1rem;
    line-height: 1.8;
  }
  #myList span {
    font-family: "courier-std", monospace;
    font-weight: 500;
    font-style: normal;
    font-size: 20px;
  }
  @media screen and (min-width: 750px) {
    .list-menu__item--link {
      padding-bottom: 0.5rem;
      padding-top: 0.5rem;
    }
  }

  /* Ed Sheeran classes */
  
  .ed-sheeran-header {
    background-color: transparent;
    transition: background-color 0.6s ease;
  }
  
  .ed-sheeran-mobile-bg {
    background-image: url('{{ 'oil-paint-BG-1200x500.png' | file_img_url: 'x600' }}');
    background-color: white;
    background-repeat: no-repeat;
    background-size: cover;
    border: 0;
    width: 100vw;
    height: 100%;
  }

  .ed-sheeran-mobile-text {
    color: black !important;
  }

  .ed-sheeran-mobile-button {
    color: black !important;
    border-color: black !important;
  }

  .ed-sheeran-mobile-flex {
    display: flex;
    justify-content: center;
  }
  
  .menu-drawer__navigation.ed-sheeran-nav-drawer {
    padding: 1.6rem 0;
  }
  
  /* Middle Center Position related classes */

  @media only screen and (min-width: 1320px) {
    #logo-h1.middle-center,
    #logo-a.middle-center {
      display: none;
    }
    
    #header-custom.middle-center {
      display: grid;
      grid-template-areas: 
        "left-icon navigation icons";
      grid-template-columns: 1fr 5fr 1fr;
      padding-bottom: 20px;
    }

    #header-custom.middle-center {
      justify-content: center;
    }
    
    #header-custom.middle-center > nav {
      justify-self: center;
      margin: 5px 0 0;
    }

    #middle-center-h1 {
      position: relative;
      top: 12px;
    }

    .search-icon {
      position: absolute;
      left: 50px;
    }

    .cart-icon {
      position: absolute;
      right: 50px;
      translate: 0px 18px;
    }

    .language-icon {
      position: absolute;
      right: 6%;
    }

    #HeaderDesktopLanguageForm > div > div > button {
      font-family: "courier-std", monospace;
      font-weight: 500;
      font-style: normal;
      font-size: 20px !important;
    }
  }

  .header-center {
      width: 60vw;
    }

    .ed-sheeran-menu-icon-hide {
      visibility: hidden !important;
    }

    #menu-drawer > div > div > div {
      margin-bottom: 5vh;
  }

  /* Tablet NavBar */
    @media only screen and (min-width: 990px) {
    #header-custom.middle-center {
      grid-template-areas: 
        "left-icon heading icons"
        "navigation navigation navigation";
      }
      #logo-h1.middle-center {
        justify-self: center;
      }
      #header-custom.middle-center > div > details-modal {
        position: absolute;
        left: 50px;
        top: 25px;
      }
      #header-custom.middle-center > div {
        justify-self: end;
      }

      #header-custom.middle-center > nav {
        justify-self: center;
      }

      #cart-icon-bubble {
        align-self: center;
      }

      #logo-a.middle-center {
        justify-self: center;
      }

      #HeaderDesktopLanguageForm > div > div > button {
        font-family: "courier-std", monospace;
        font-weight: 500;
        font-style: normal;
        font-size: 20px !important;
      }
    }

  /* Mobile NavBar */
  @media only screen and (max-width: 990px) {
    #header-custom.middle-center {
      grid-template-areas: "icons heading left-icon";
    }
    .header__icons {
      justify-self: start;
    }
    #header-custom > div {
      flex-direction: row-reverse;
    }
  }
  
  /* The sticky class is added to the header with JS when it reaches its scroll position */
  .sticky {
    position: fixed;
    top: 0;
    width: 100%
  }

</style>

