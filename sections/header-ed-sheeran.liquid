                    {%- endif -%}
                    {%- if settings.social_tumblr_link != blank -%}
                      <li class="list-social__item">
                        <a href="{{ settings.social_tumblr_link }}" class="link list-social__link {% if shop.name == 'Ed Sheeran' %}ed-sheeran-mobile-text{% endif %}" aria-describedby="a11y-external-message">
