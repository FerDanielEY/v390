  <script>
    // Change header background-color at scroll
   var header_bg_color = document.querySelector('.ed-sheeran-header');
   window.addEventListener('scroll', function() {
     var scroll = window.scrollY;
     if (scroll > 100) {
       header_bg_color.style.backgroundColor = '#EEE9E2';
     } else {
       header_bg_color.style.backgroundColor = 'transparent';
     }
   });
    // Add the sticky class to the header when you reach its scroll position. Remove "sticky" when you leave the scroll position.
    window.onscroll = function() {stickyNav()};
    var header = document.getElementById("ed-sheeran-sticky-header");
    var sticky = header.offsetTop; // Get the offset position of the navbar 
    function stickyNav() {
      if (window.pageYOffset > sticky) {
        header.classList.add("sticky");
      } else {
        header.classList.remove("sticky");
      }
    }
    // Change header background when menu button is clicked.
    var details = document.querySelector('details');
    details.addEventListener("toggle", function () {
      // Add the required class
      var open = document.querySelector('details').classList.contains('menu-opening');
